<Application x:Class="MVVM_XML_RS.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:MVVM_XML_RS"
             StartupUri="Views\Windows\MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles/Buttons.xaml"/>
                <ResourceDictionary Source="Resources/Styles/Tabitems.xaml"/>
                <ResourceDictionary Source="Resources/Styles/Headers.xaml"/>
                <ResourceDictionary Source="Resources/Styles/DataGridS.xaml"/>
                <ResourceDictionary Source="Resources/Styles/Border.xaml"/>
                <ResourceDictionary Source="Resources/Styles/ListBoxItem.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
